{
  "name": "mastering-ai-course-portfolio",
  "version": "0.1.0",
  "private": true,
  "description": "AI portfolio showcasing 6 cutting-edge projects for Mastering Generative AI bootcamp",
  "author": "Tom Butler",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "dev:dashboard": "turbo dev --filter=dashboard",
    "build": "turbo build",
    "test": "turbo test",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "turbo type-check"
  },
  "devDependencies": {
    "@types/node": "^22.9.3",
    "eslint": "^9.16.0",
    "prettier": "^3.4.2",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "packageManager": "npm@10.9.2",
  "engines": {
    "node": ">=20.0.0"
  }
}